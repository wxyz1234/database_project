    6        | SHOW DATABASE NAME
    7        | SHOW DATABASES
    8        | DESC NAME
    9        | CREATE TABLE NAME '(' attributelist ')'
   10        | DROP TABLE NAME
   11        | SHOW TABLE NAME
   12        | SHOW TABLES
   13        | CREATE INDEX NAME ON NAME '(' NAME ')'
