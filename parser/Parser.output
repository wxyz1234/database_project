Grammar

    0 $accept: sql $end

    1 sql: command ';'
    2    | sql command ';'

    3 command: CREATE DATABASE NAME
    4        | DROP DATABASE NAME
    5        | USE DATABASE NAME
    6        | SHOW DATABASE NAME
    7        | SHOW DATABASES
    8        | DESC DATABASE NAME
    9        | CREATE TABLE NAME '(' attributelist ')'
   10        | DROP TABLE NAME
   11        | SHOW TABLE NAME
   12        | DESC TABLE NAME
   13        | SHOW TABLES
   14        | CREATE INDEX NAME ON NAME '(' NAME ')'
   15        | DROP INDEX NAME
   16        | ALTER TABLE NAME ADD attribute
   17        | ALTER TABLE NAME DROP PRIMARY KEY
   18        | ALTER TABLE NAME DROP FOREIGN KEY '(' NAME ')'
   19        | ALTER TABLE NAME DROP NAME
   20        | ALTER TABLE NAME RENAME TO NAME
   21        | INSERT INTO NAME VALUES valuelists
   22        | DELETEC FROM NAME whereClauses
   23        | UPDATE NAME SET setClauselist whereClauses
   24        | SELECT columnlist FROM tablelist whereClauses
   25        | SELECT '*' FROM tablelist whereClauses

   26 whereClauses: WHERE conditions
   27             | /* empty */

   28 conditions: comparison
   29           | conditions AND comparison

   30 comparison: column op expr
   31           | column IS NULLC
   32           | column IS NOT NULLC

   33 tablelist: NAME
   34          | tablelist ',' NAME

   35 columnlist: column
   36           | columnlist ',' column

   37 column: NAME '.' NAME
   38       | NAME

   39 expr: value
   40     | column
   41     | expr '+' expr
   42     | expr '-' expr
   43     | expr '*' expr
   44     | expr '/' expr
   45     | expr '%' expr

   46 op: EQ
   47   | GT
   48   | LT
   49   | GE
   50   | LE
   51   | NE

   52 setClauselist: setClause
   53              | setClauselist ',' setClause

   54 setClause: column '=' expr

   55 attributelist: attribute
   56              | attributelist ',' attribute

   57 attribute: NAME type
   58          | NAME type NOT NULLC
   59          | NAME type DEFAULT value
   60          | NAME type NOT NULLC DEFAULT value
   61          | PRIMARY KEY '(' NAME ')'
   62          | FOREIGN KEY '(' NAME ')' REFERENCES NAME '(' NAME ')'

   63 type: INTEGER '(' INUM ')'
   64     | SMALLINT
   65     | CHARTYPE '(' INUM ')'
   66     | DOUBLETYPE
   67     | FLOATTYPE
   68     | DATETYPE
   69     | NUMERICTYPE '(' INUM ',' INUM ')'

   70 valuelists: '(' valuelist ')'
   71           | valuelists ',' '(' valuelist ')'

   72 valuelist: value
   73          | valuelist ',' value

   74 value: TEXT
   75      | INUM
   76      | FNUM
   77      | DATENUM


