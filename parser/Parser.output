   40     | expr '+' expr
   41     | expr '-' expr
   42     | expr '*' expr
   43     | expr '/' expr
   44     | expr '%' expr

   45 op: EQ
   46   | GT
   47   | LT
   48   | GE
   49   | LE
   50   | NE

   51 setClauselist: setClause
   52              | setClauselist ',' setClause

   53 setClause: column '=' expr

   54 attributelist: attribute
   55              | attributelist ',' attribute

   56 attribute: NAME type
   57          | NAME type NOT NULLC
   58          | NAME type DEFAULT value
   59          | NAME type NOT NULLC DEFAULT value
   60          | PRIMARY KEY '(' NAME ')'
   61          | FOREIGN KEY '(' NAME ')' REFERENCES NAME '(' NAME ')'

   62 type: INTEGER '(' INUM ')'
   63     | SMALLINT
   64     | CHARTYPE '(' INUM ')'
   65     | DOUBLETYPE
   66     | FLOATTYPE
   67     | DATETYPE
   68     | NUMERICTYPE '(' INUM ',' INUM ')'

   69 valuelists: '(' valuelist ')'
