   18        | ALTER TABLE NAME DROP NAME
   19        | ALTER TABLE NAME RENAME TO NAME
   20        | INSERT INTO NAME VALUES valuelists
   21        | DELETEC FROM NAME whereClauses
   22        | UPDATE NAME SET setClauselist whereClauses
